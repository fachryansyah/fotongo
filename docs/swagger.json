{
    "swagger": "2.0",
    "info": {
        "description": "An API Documentation for Payers app",
        "title": "Fotongo REST API",
        "termsOfService": "http://swagger.io/terms/",
        "contact": {
            "name": "API Support",
            "email": "fiber@swagger.io"
